
import java.util.*;
class Main {
    public static void main(String[] args) {
      int n=1101;
      String strNumber1 = String.valueOf(n);
       int l = strNumber1.length();
       int i=0;
       double res=0;
      while(i<l){
          res=res+(n%2)*Math.pow(2,i);
          i++;
          n/=2;
      }
      System.out.println(res);
       
    }
}
