import java.util.*;
class Main {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       int start=sc.nextInt();
       int goal=sc.nextInt();
       int count=0;
       int ans=start^goal;
      for(int i=0;i<32;i++){
           if((ans&(1<<i))!=0)
           count+=1;
          
       }
       System.out.println(count);
       
    }      
}
